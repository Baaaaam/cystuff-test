<!-- 1 Source Source, 1 Sink Sink -->

<simulation>
  <schematype>flat</schematype>

  <control>
    <duration>1200</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>

  <archetypes>
    <spec><lib>cycamore</lib><name>Sink</name></spec>
    <spec> <lib>cycamore</lib> <name>DeployInst</name></spec>
    <spec><lib>cycamore</lib><name>Source</name></spec>
    <spec><lib>cycamore</lib><name>Reactor</name></spec>
    <spec><lib>cycamore</lib><name>Storage</name></spec>
    <spec><lib>cycamore</lib><name>Separations</name></spec>
    <spec><lib>cycamore</lib><name>Enrichment</name></spec>
    <spec><lib>cycamore</lib><name>Reactor</name></spec>
    <spec><lib>cyclass</lib><name>Reactor</name><alias>cyclassReactor</alias></spec>
    <spec><lib>cyclass</lib><name>FuelFab</name><alias>cyclassFuelFab</alias></spec>
  </archetypes>

  <prototype>
    <name>SourceUOX</name>
    <config>
      <Source>
        <outcommod>Spent_UOXstored</outcommod>
        <outrecipe>SpentUOX</outrecipe>
        <throughput>1000000</throughput>
      </Source>
    </config>
  </prototype>

  <prototype>
    <name>depleted_src</name>
    <config>
      <Source>
        <outcommod>depleted_u</outcommod>
        <outrecipe>depleted_u</outrecipe>
        <throughput>1e100</throughput>
      </Source>
    </config>
  </prototype>


  <prototype>
    <name>Fab</name>
    <config>
      <cyclassFuelFab>

        <eq_model>PWR_MLP_MOX</eq_model>
        <eq_command>/Users/mouginot/scratch/App/CLASS/Database/PWR/MOX/EQModel/MLP/EQM_MLP_PWR_MOX_3batch.xml</eq_command>

        <fill_commods><val>depleted_u</val></fill_commods>
        <fill_recipe>depleted_u</fill_recipe>
        <fill_size>1000000000</fill_size>


        <fiss_commods>
          <val>UOX_pu</val>
        </fiss_commods>
        <fiss_commod_prefs>
          <val>1.0</val>
        </fiss_commod_prefs>
        <fiss_size>1000000000</fiss_size>

        <throughput>1e100</throughput>
        <outcommod>MOX</outcommod>
      </cyclassFuelFab>
    </config>
  </prototype>

  <prototype>
    <name>ReactorMOX</name>
    <config>
      <cyclassReactor>
        <eq_model>PWR_MLP_MOX</eq_model>
        <eq_command>/Users/mouginot/scratch/App/CLASS/Database/PWR/MOX/EQModel/MLP/EQM_MLP_PWR_MOX_3batch.xml</eq_command>

        <ir_model>RK4</ir_model>
        <ir_command>none</ir_command>

        <xs_model>XSM_MLP</xs_model>
        <xs_command>/Users/mouginot/scratch/App/CLASS/Database/PWR/MOX/XSModel/30Wg_FullMOX/,Data_Base_Info.nfo,false</xs_command>



        <fuel_incommods>	<val>MOX</val>		</fuel_incommods>
        <fuel_outcommods>	<val>SpentMOX</val>	</fuel_outcommods>
        <burnup>50</burnup>

        <cycle_time>36</cycle_time>
        <power_cap>965.16</power_cap>
        <power>2700</power>
        <refuel_time>0</refuel_time>
        <assem_size>82</assem_size>
        <n_assem_core>1</n_assem_core>
        <n_assem_spent>1</n_assem_spent>
        <n_assem_batch>1</n_assem_batch>
      </cyclassReactor>
    </config>
  </prototype>



  <prototype>
    <name>UOX_Storage</name>
    <config>
      <Storage>
        <in_commods><val>SpentUOX</val></in_commods>
        <out_commods><val>Spent_UOXstored</val></out_commods>
        <residence_time>1</residence_time>
      </Storage>
    </config>
  </prototype>

  <prototype>
    <name>waste</name>
    <config>
      <Sink>
        <in_commods><val>waste</val></in_commods>
        <capacity>1e100</capacity>
      </Sink>
    </config>
  </prototype>




  <prototype>
    <name>LWR_separation</name>
    <config>
      <Separations>
        <leftover_commod>waste</leftover_commod>
        <throughput>83333.3333</throughput>
        <feedbuf_size>107537</feedbuf_size>
        <feed_commods>
          <val>Spent_UOXstored</val>
        </feed_commods>
        <feed_commod_prefs>
          <val>1.0</val>
        </feed_commod_prefs>
        <streams>
          <item>
            <commod>UOX_pu</commod>
            <info>
              <buf_size>1e100</buf_size>
              <efficiencies>
                <item> <comp>Pu</comp> <eff>.99</eff> </item>
              </efficiencies>
            </info>
          </item>
          <item>
            <commod>RU</commod>
            <info>
              <buf_size>1e100</buf_size>
              <efficiencies>
                <item> <comp>U</comp> <eff>.99</eff> </item>
              </efficiencies>
            </info>
          </item>
        </streams>

      </Separations>
    </config>
  </prototype>

  <prototype>
    <name>deployer</name>
    <config>
      <DeployInst>
        <prototypes>
          <val>LWR_separation</val>
          <val>waste</val>
          <val>SourceUOX</val>
          <val>depleted_src</val>
          <val>UOX_Storage</val>
          <val>ReactorMOX</val>
          <val>Fab</val>

        </prototypes>

        <build_times>
          <val>1</val>
          <val>1</val>
          <val>1</val>
          <val>1</val>
          <val>1</val>
          <val>120</val>
          <val>1</val>
        </build_times>

        <n_build>
          <val>1</val>
          <val>1</val>
          <val>1</val>
          <val>1</val>
          <val>1</val>
          <val>1</val>
          <val>1</val>
        </n_build>
      </DeployInst>
    </config>
  </prototype>


  <agent> <name>Deplo</name> <prototype>deployer</prototype> </agent>


  <recipe>
    <name>UOX</name>
    <basis>mass</basis>
    <nuclide>
      <id>922350000</id>
      <comp>3.25</comp>
    </nuclide>
    <nuclide>
      <id>922380000</id>
      <comp>96.75</comp>
    </nuclide>
    <nuclide>
      <id>80160000</id>
      <comp>200</comp>
    </nuclide>
  </recipe>

  <recipe>
    <name>depleted_u</name>
    <basis>mass</basis>
    <nuclide> <id>U235</id><comp>0.0025</comp></nuclide>
    <nuclide><id>U238</id><comp>0.9975</comp></nuclide>
  </recipe>


  <recipe>
    <name>SpentUOX</name>
    <basis>atom</basis>
    <nuclide><id>922350000</id>	<comp>0.5248</comp></nuclide>
    <nuclide><id>922380000</id>	<comp>89.3300</comp></nuclide>
    <nuclide><id>942380000</id>	<comp>0.0229</comp></nuclide>
    <nuclide><id>942390000</id>	<comp>0.5429</comp></nuclide>
    <nuclide><id>942400000</id>	<comp>0.2472</comp></nuclide>
    <nuclide><id>942410000</id>	<comp>0.1592</comp></nuclide>
    <nuclide><id>942420000</id>	<comp>0.0825</comp></nuclide>
    <nuclide><id>952420000</id>	<comp>0.0040</comp></nuclide>
  </recipe>
  
</simulation>






